*
  margin 0
  padding 0
  box-sizing border-box
  font-size 14px
  font-family "Helvetica Neue", Helvetica, Arial, sans-serif
  color #333333
body, html, #app
  color #333333
  background #fff
  font-size 14px
  height 100%
  .index_wrapper
    width 100%
    min-width 1200px
    height 100%
input
  outline none
  border none
  border 1px solid #acbaca
  font-size 14px
  border-radius 5px
  padding-left 5px
input::-webkit-input-placeholder
  color #666e79
button,select
  outline none
  border none
placeholder
  color #666e79
select
  appearance none
  -webkit-appearance none
  background none
  border 1px solid #acbaca
ul,li
  list-style none
a
  text-decoration none
table
  border-collapse collapse
  border-spacing 0
.blue-btn
  margin auto
  width 80px
  height 30px
  line-height 30px
  padding 0 20px
  border-radius 5px
  letter-spacing 5px
  color #fff
  background rgba(22, 155, 213, 1)
  box-shadow 0 5px 8px 0 rgba(22, 155, 213, 0.5)
  cursor pointer
iframe
  display none






